<mapper namespace="com.qstudy.qstudy.mapper.login.GithubLoginMapper">
	<!-- 깃허브 로그인 데이터 저장 -->
  <insert id="saveGithub" parameterType="com.qstudy.qstudy.dto.login.User">
	  INSERT INTO userprofile(id, email, nickname) VALUES (#{id},#{email},#{nickname});
  </insert>
</mapper>
